LICENSE: -L --license "print license information.";
VERSION: -V --version "print version.";
HELP:    -h --help    "print this message.";

STDOUT:
    -t --stdout
    "write parser to stdout.";

OUTPUT:
    -o --output
    "specify output filename."
    =;

HEADER:  
    -H --header-file 
    "create a C header file in addition to the parser."
    = "gonf.h";

PREFIX:
    -p --prefix
    "specify a prefix to use instead of \"gonf\"."
    =;
